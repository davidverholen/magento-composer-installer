{
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        }
    ],
    "require": {
        "aoepeople/aoe_scheduler": "@stable",
        "bragento/magento-composer-installer": "dev-develop",
        "magento/core": "*",
        "theseer/autoload": "~1.14",
        "firegento/magesetup": "@stable",
        "firegento/debug": "@stable",
        "zetacomponents/console-tools": "dev-master",
        "firegento/psr0autoloader": "dev-master",
        "connect20/aw_blog": "*",
        "connect20/aw_onpulse": "*",
        "connect20/aw_aheadmetrics": "*",
        "connect20/aw_fps": "*"
    },
    "extra": {
        "magento-force": "override",
        "magento-deploystrategy-overwrite": {
            "firegento/magesetup": "copy"
        },
        "auto-append-gitignore": true
    }
}
